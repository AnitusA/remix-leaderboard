{
  "name": "bytebashblitz",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "dev:debug": "remix vite:dev --debug",
    "build:analyze": "npm run build && npx vite-bundle-analyzer build/client",
    "db:migrate": "echo 'Run migrations in Supabase dashboard from supabase/migrations/'",
    "db:sync": "./scripts/db-sync.sh",
    "db:create-migration": "./scripts/db-sync.sh create",
    "db:check": "./scripts/db-sync.sh check",
    "db:remote": "./scripts/db-sync.sh remote",
    "db:dump": "./scripts/db-sync.sh dump",
    "db:docs": "./scripts/db-sync.sh docs",
    "types:generate": "./scripts/types-generator.sh generate",
    "types:check": "./scripts/types-generator.sh manual",
    "types:help": "./scripts/types-generator.sh help",
    "clean": "rm -rf build node_modules/.cache",
    "prepare": "npm run typecheck && npm run lint && npm run build"
  },
